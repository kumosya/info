#ifndef LIBCPP_INITIALIZER_LIST_H
#define LIBCPP_INITIALIZER_LIST_H

namespace std {
template <class E> class initializer_list {
  const E* _M_array;
  size_t _M_len;
public:
  constexpr initializer_list() noexcept : _M_array(nullptr), _M_len(0) {}
  constexpr initializer_list(const E* a, size_t n) noexcept : _M_array(a), _M_len(n) {}
  constexpr size_t size() const noexcept { return _M_len; }
  constexpr const E* begin() const noexcept { return _M_array; }
  constexpr const E* end() const noexcept { return _M_array + _M_len; }
};
}

#endif // LIBCPP_INITIALIZER_LIST_H
