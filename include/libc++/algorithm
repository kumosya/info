#ifndef LIBCPP_ALGORITHM_H
#define LIBCPP_ALGORITHM_H

#include <utility>

namespace std {

template <class It, class T>
void fill(It first, It last, const T& value) {
    for (; first != last; ++first) *first = value;
}

template <class It1, class It2>
It2 copy(It1 first, It1 last, It2 out) {
    for (; first != last; ++first, ++out) *out = *first;
    return out;
}

template <class T>
const T& min(const T& a, const T& b) { return (b < a) ? b : a; }

} // namespace std

#endif // LIBCPP_ALGORITHM_H
