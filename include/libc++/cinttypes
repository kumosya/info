#ifndef LIBCPP_CINTTYPES_H
#define LIBCPP_CINTTYPES_H

/* Minimal <cinttypes> for freestanding builds.
   Includes <inttypes.h> and exposes a couple of commonly used
   types into namespace std. */

#include <inttypes.h>

#ifdef __cplusplus
namespace std {
using ::intmax_t;
using ::uintmax_t;
}
#endif /* __cplusplus */

#endif /* LIBCPP_CINTTYPES_H */
